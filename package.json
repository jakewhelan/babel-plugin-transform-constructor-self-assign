{
  "name": "@jakewhelan/babel-plugin-transform-constructor-self-assign",
  "version": "0.0.1",
  "description": "Babel plugin to automagically assign ES2015 class constructor arguments to this, as seen in Typescript",
  "main": "./dist/index.js",
  "scripts": {
    "build": "set BABEL_ENV=plugin&& babel index.js --out-file ./dist/index.js",
    "sample": "set BABEL_ENV=sample&& babel sample.js --out-file ./dist/sample.js"
  },
  "author": "Jake Whelan",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1"
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1"
  }
}
